<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" Width="24" Height="24" Cursor="DragMove"
             x:Class="AvaloniaApplication1.Connector">
  <Canvas>
    <Ellipse Fill="Gray" Width="22" Height="22" Canvas.Left="1" Canvas.Top="1"/>
    <Ellipse Fill="DarkGray" Width="18" Height="18" Canvas.Left="3" Canvas.Top="3"/>
    <Label HorizontalAlignment="Center" Margin="5,3,0,0">^</Label>
  </Canvas>
</UserControl>
